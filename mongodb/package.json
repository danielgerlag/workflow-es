    "@types/node": "^6.0.51",
    "@types/jasmine": "^2.5.38",
    "jasmine": "^2.5.2",
    "jasmine-core": "^2.4.1",
    "typescript": "^2.2.1"
  },
  "dependencies": {
    "inversify": "^4.1.0",
    "json-stable-stringify": "^1.0.1",
    "mongodb": "^2.2.11",
    "reflect-metadata": "^0.1.10",
    "workflow-es": "^2.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danielgerlag/workflow-es.git"
